# refine domains
rule refine_domains:
    input:
        domains='{output_dir}/domains_by_sample/{bin_size}/{pvalue}/{sample_id}.bed',
        tbigwig='{output_dir}/tbigwig_normalized/{sample_id}.transcriptome.bigWig',
        gbigwig_plus='{output_dir}/bigwig_normalized/{sample_id}.other.+.bigWig',
        gbigwig_minus='{output_dir}/bigwig_normalized/{sample_id}.other.-.bigWig',

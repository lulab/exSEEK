        flanking=20
    shell:
        '''bin/call_peak.py refine_peaks --peaks {input.domains} \
            --tbigwig {input.tbigwig} \
            --gbigwig-plus {input.gbigwig_minus} \
            --gbigwig-minus {input.gbigwig_plus} \
            --chrom-sizes {input.chrom_sizes}  \
